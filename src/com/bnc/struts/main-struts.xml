<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
    <package name="main" namespace="/main" extends="struts-default">
        <action name="noticelist" class="com.bnc.action.MainAction" method="executeNoticeList">
            <result>/pages/main/noticelist.jsp</result>
        </action>
        <action name="bestcarlist" class="com.bnc.action.MainAction" method="executeBestCarList">
            <result>/pages/main/bestcarlist.jsp</result>
        </action>
        <action name="besttravellist" class="com.bnc.action.MainAction" method="executeBestTravelList">
            <result>/pages/main/besttravellist.jsp</result>
        </action>
        <action name="besttravellist" class="com.bnc.action.MainAction" method="executeBestTravelList">
            <result>/pages/main/besttravellist.jsp</result>
        </action>
        <action name="carlist" class="com.bnc.action.MainAction" method="executeCarList">
            <result>/pages/main/carlist.jsp</result>
        </action>
        <action name="travellist" class="com.bnc.action.MainAction" method="executeTravelList">
            <result>/pages/main/travellist.jsp</result>
        </action>
    </package>     
</struts>
